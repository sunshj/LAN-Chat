{
  "name": "lan-chat-server",
  "version": "1.4.5",
  "type": "module",
  "license": "MIT",
  "author": "sunshj",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2",
    "h3": "1.12.0",
    "h3-formidable": "^1.0.0",
    "mrmime": "^2.0.0",
    "socket.io": "4.7.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/formidable": "^3.4.5"
  }
}
